pipeline{
    agent any

    stages{
   
        stage('Maven Build'){
            steps{
                sh "mvn clean package"
            }
        }
        
       stage('Build'){
            steps{
                sh 'mvn clean package'
            }
         }
        stage('SonarQube analysis') {
        steps{
        withSonarQubeEnv('sonarqube-9.6.1') { 
        // If you have configured more than one global server connection, you can specify its name
        sh "mvn sonar:sonar"
    }
        }
        }
       
    }
}
